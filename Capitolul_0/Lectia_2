#rotunjire
c = 6.51 # ca sa fie rotunjit trebuie sa fie mai mult de 0,51 integi
print('rotunjire: ' + str(round(c)))
print('============================================')
#import functii
import math

#rotujnire spre

print('rotunjire' + str(math.floor(c)))
print('============================================')

#rotunjire spre mare

print('rortunjire; ' + str(math.ceil(c)))
print('============================================')

#rotunjire pi

print('rotunjire;' + str(math.pi))
print('============================================')

