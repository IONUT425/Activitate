# Math !!!! #Radacina patratata(radical) importam din biblioteca matematica
#ca sa nu apara 0 scrii intregi la number
#steluta face operatiile

from math import *
number = 144
print("radacina", number, "=", sqrt(number))
print('============================================')


#functie

def cmmnc (a,b) :
    return a if b==0 else cmmnc (b, a%b)

c=cmmnc(48,24)
print('c=' + str(c))
print('============================================')

c=cmmnc(72,12)
print('c=' + str(c))
print('============================================')

c=cmmnc(96,32)
print('c=' + str(c))
print('============================================')

def fibonacci(n) :
    if n==1:
        return 1
    elif n==2:
        return 2
    elif n>2:
        return fibonacci(n-1)+fibonacci(n-2)

for n in range(1,19):
    print (n,":", fibonacci(n))
print('============================================')

#functia lui fibonnacci

print(1,":", fibonacci(1))
print(2,":", fibonacci(2))
print(3,":", fibonacci(3))
print(4,":", fibonacci(4))
print(5,":", fibonacci(5))
print(6,":", fibonacci(6))
print(7,":", fibonacci(7))
print(8,":", fibonacci(8))
print(9,":", fibonacci(9))

print(10, ",:", fibonacci(10))
print(11, ",:", fibonacci(11))
print(12, ",:", fibonacci(12))
print(13, ",:", fibonacci(13))
print(14, ",:", fibonacci(14))
print(15, ",:", fibonacci(15))
print(16, ",:", fibonacci(16))
print(17, ",:", fibonacci(17))
print(18, ",:", fibonacci(18))
print(19, ",:", fibonacci(19))

